<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ourteam.covid_19">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.ourteam.covid_19.Emergency"></activity>
        <activity android:name="com.ourteam.covid_19.RiskTest" />

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-9189255863868239~2015463408" />

        <activity android:name="com.ourteam.covid_19.NewsWeb" />
        <activity android:name="com.ourteam.covid_19.FeedBack" />
        <activity android:name="com.ourteam.covid_19.AboutUs" />
        <activity android:name="com.ourteam.covid_19.News" />
        <activity android:name="com.ourteam.covid_19.TestOrHospital" />
        <activity android:name="com.ourteam.covid_19.VaccineCenter" />
        <activity android:name="com.ourteam.covid_19.VaccineActivity" />
        <activity android:name="com.ourteam.covid_19.HandClean" />
        <activity android:name="com.ourteam.covid_19.Information" />
        <activity android:name="com.ourteam.covid_19.Language" />
        <activity android:name="com.ourteam.covid_19.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ourteam.covid_19.MainActivity" />

        <service android:name="com.ourteam.covid_19.NotificationIdService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>